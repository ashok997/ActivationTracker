<h1> New Phone ! </h1>

<%= form_for(@phone) do |f| %>
    <%= f.label :model %>
    <%= f.text_field :model %>
    <br>
    <br>
    <%= f.label :manufacturer %>
    <%= f.text_field :manufacturer %>
    <br>
    <br>
    <%= f.label :storage %>
    <%= f.text_field :storage %>

    <%= f.submit %>
<% end %>