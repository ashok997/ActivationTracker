<h1> Sign UP! </h1>
<%= render partial: "layouts/errors", locals: {object: @user} %>
<%= form_for(@user, url: '/signup') do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Submit" %>


<% end %>